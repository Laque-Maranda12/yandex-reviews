APP_NAME=DailyGrow
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://yandex-reviews-production.up.railway.app
ASSET_URL=https://yandex-reviews-production.up.railway.app
LOG_CHANNEL=stack
LOG_LEVEL=debug
DB_CONNECTION=sqlite
DB_DATABASE=/app/database/database.sqlite
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120
SANCTUM_STATEFUL_DOMAINS=yandex-reviews-production.up.railway.app
SESSION_DOMAIN=yandex-reviews-production.up.railway.app
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=none

# Yandex Maps API token for organization search by name
# Get it at https://developer.tech.yandex.ru/ (API для поиска по организациям)
YANDEX_MAPS_API_TOKEN=

# Yandex Reviews: Proxy list (comma-separated)
# Rotated between branches to avoid captcha
# Formats: http://user:pass@host:port, socks5://host:port
YANDEX_PROXIES=

# Captcha solving service (rucaptcha.com or 2captcha.com)
CAPTCHA_API_KEY=
CAPTCHA_API_URL=https://rucaptcha.com
